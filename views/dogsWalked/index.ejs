<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Home Page</title>
</head>
<body>
    <h1>Dogs Walked</h1>
    <ul>
        <% dogsWalked.forEach(function(dogs) { %>
            <li>
                <%= dogs.dog %>
                - 
                <%= dogs.done ? 'walked' : 'not walked' %>
            </li>
        <% }) %>
    </ul>
</body>
</html>